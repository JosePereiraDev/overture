commit b0c45d0d1732b4906cba7dcf377d50dfe4803e88
Author: Peter W. V. Joergensen <pvj@iha.dk>
Date:   Thu Apr 18 14:39:30 2013 +0200

    Bug fix for parsing of "is not yet specified" and "is subclass responsibility". Prior to this fix the parser did not give any errors for models like:
    
    \begin{vdm_al}
    class Event
    values public id : token = is subclass responsibility
    end Event
    \end{vdm_al}
    
    \begin{vdm_al}
    class InitEvent is subclass of Event
    values public id : token = mk_token("INIT")
    end InitEvent
    \end{vdm_al}
    
    \begin{vdm_al}
    class EntryEvent is subclass of Event
    values public id : token = mk_token("ENTRY")
    end EntryEvent
    \end{vdm_al}
    
    \begin{vdm_al}
    class ExitEvent is subclass of Event
    values public id : token = mk_token("EXIT")
    end ExitEvent
    \end{vdm_al}

