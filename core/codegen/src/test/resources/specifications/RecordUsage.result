import org.overture.codegen.runtime.*;

import java.util.*;


public class A {
    private static final R2 a = new R2(new R1(2L), 3L);
    private static final R2 b = a.clone();
    private static final R1 c = a.r1.clone();
    private static final Number d = a.r1.x;

    public A() {
    }

    public Number op1() {
        R1 r = new R1(2L);
