import org.overture.codegen.runtime.*;

import java.util.*;


public class A {
    public static final Boolean a = new R(1L).equals(new R(2L));
    public static final Boolean b = !(new R(1L).equals(new R(2L)));

    public A() {
    }

    public static class R implements Record {
        public Number x;

        public R(final Number _x) {
            x = _x;
        }

        public boolean equals(final Object obj) {
            if (!(obj instanceof R)) {
                return false;
            }

            R other = ((R) obj);

            return x.longValue() == other.x.longValue();
        }

        public int hashCode() {
            return Utils.hashCode(x);
        }

        public R clone() {
            return new R(x);
        }

        public String toString() {
            return Utils.recordToString(this, x);
        }
    }
}

